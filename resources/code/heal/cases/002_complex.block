(ns sznui.v1.screens.demo.quickstart-page-demo.detailed-guides
  (:require [std.lang :as l]
            [std.lib :as h]))

(l/script :js
  {:runtime :websocket
   :require [[js.react :as r]
             [js.tamagui :as tm]
             [xt.lang.base-lib :as k]]
   :import  [["@tamagui/lucide-icons"
              :as #{CheckCircle2 Clock Users Coins Target BarChart3 Zap Copy ExternalLink BookOpen Play Code Rocket Settings Share2 Download}]]
   :export [MODULE]})

(defn.js QuickstartDetailedGuides

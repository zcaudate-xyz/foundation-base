             "4"]
            [:% tm/View {:flex 1}
             [:% tm/Text {:tag "h4" :fontSize "$4" :fontFamily "$heading" :marginBottom "$2"} "Configure Rewards"]
             [:% tm/Text {:marginBottom "$3" :fontSize "$sm" :color "$color10"}

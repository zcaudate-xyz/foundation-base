           [:% tm/View {:display "flex" :gap "$4"}
            [:% tm/View {:display "flex" :height "$4" :width "$4" :flexShrink 0 :alignItems "center" :justifyContent "center" :borderRadius "$full" :backgroundColor "$primary" :color "$primaryForeground"}

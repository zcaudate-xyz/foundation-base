                [:% tm/Text {:color "$primary"} "50 tokens"]] 
               [:% tm/View {:display "flex" :justifyContent "space-between"}
                [:% tm/Text "Winner Pool:"]
                [:% tm/Text {:color "$primary"} "10,000 tokens"]] 
               [:% tm/View {:display "flex" :justifyContent "space-between"}
                [:% tm/Text "Total Budget:"]
                [:% tm/Text {:color "$primary"} "25,000 tokens"]]]]]

           [:% tm/Separator]

           ;; Step 5
           [:% tm/View {:display "flex" :gap "$4"}
            [:% tm/View {:display "flex" :height "$4" :width "$4" :flexShrink 0 :alignItems "center" :justifyContent "center" :borderRadius "$full" :backgroundColor "$primary" :color "$primaryForeground"}
             "5"]
            [:% tm/View {:flex 1}
             [:% tm/Text {:tag "h4" :fontSize "$4" :fontFamily "$heading" :marginBottom "$2"} "Launch & Share"]
             [:% tm/Text {:marginBottom "$3" :fontSize "$sm" :color "$color10"}

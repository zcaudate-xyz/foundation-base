         [:% tm/View {:borderBottomWidth 1 :borderBottomColor "$border" :padding "$6"}
          [:% tm/View {:display "flex" :alignItems "flex-start" :justifyContent "space-between"}
           [:% tm/View
            [:% tm/Text {:tag "h2" :fontSize "$5" :fontFamily "$heading" :marginBottom "$2"} "Create a Promotional Token"]

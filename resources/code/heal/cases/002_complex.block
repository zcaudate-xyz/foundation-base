             "Design your custom token in 4 easy steps"]] 
           [:% tm/Badge {:variant "secondary"} "8-12 min"]]]

         [:% tm/View {:padding "$6"}
          [:% tm/View {:gap "$6"}
           ;; Step 1
           [:% tm/View {:display "flex" :gap "$4"}
            [:% tm/View {:display "flex" :height "$4" :width "$4" :flexShrink 0 :alignItems "center" :justifyContent "center" :borderRadius "$full" :backgroundColor "$primary" :color "$primaryForeground"}
             "1"]
            [:% tm/View {:flex 1}
             [:% tm/Text {:tag "h4" :fontSize "$4" :fontFamily "$heading" :marginBottom "$2"} "Open Token Creator"]
             [:% tm/Text {:marginBottom "$3" :fontSize "$sm" :color "$color10"}
              "Navigate to the Token Creator tool from your dashboard."]
             [:% tm/Button {:variant "outline"
                            :size "sm"
                            :onPress (fn [] (onNavigate "token-creator"))}
